{
    "git.openRepositoryInParentFolders": "always",
    "sqltools.sessionFilesFolder": ".vscode/.sqltools",
    "cSpell.languageSettings": [
        {
            "languageId": "en",
            "allowCompoundWords": true
        },
        {
            "languageId": "es",
            "allowCompoundWords": true
        }
    ],
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "cSpell.words": [
        "DTOREST",
        "fkey",
        "ILIKE",
        "imageable",
        "ossp",
        "webui"
    ]
}